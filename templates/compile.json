{
  "targets": [
    {
      "type": "source",
      "outputPath": "source-output"
    },
    {
      "type": "hybrid",
      "outputPath": "hybrid-output"
    },
    {
      "type": "build",
      "outputPath": "build-output"
    }
  ],
  "defaultTarget": "source",
  "locales": [ "en" ],

  "applications": [
    {
      "class": "$namespace.Application",
      "theme": "$namespace.theme.Theme",
      "name": "$namespace"
    }
  ],

  "libraries": [
    {
      "path": "$qxpath/framework"
    },
    {
      "path": "."
    }
  ]
}